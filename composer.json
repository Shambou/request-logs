{
    "name": "shambou/request-logs",
    "description": "Store request/response data and headers from receiving API calls",
    "license": "MIT",
    "authors": [
        {
            "name": "Sasa Milasinovic",
            "email": "sasa.milasinovic@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/contracts": "^8.0",
        "ext-json": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Shambou\\RequestLogs\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Shambou\\RequestLogs\\Tests\\": "tests",
            "Shambou\\RequestLogs\\Database\\Factories\\": "database/factories"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-f": "vendor/bin/phpunit --filter",
        "cs:fix": "./vendor/bin/php-cs-fixer fix"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shambou\\RequestLogs\\RequestLogServiceProvider"
            ],
            "aliases": {
                "RequestLogFactory": "JohnDoe\\BlogPackage\\Facades\\RequestLogFactory",
                "RequestLogParserFactory": "JohnDoe\\BlogPackage\\Facades\\RequestLogParserFactory"
            }
        }
    }
}
